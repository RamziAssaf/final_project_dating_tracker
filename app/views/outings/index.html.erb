<div class="page-header">
  <h1>Outings</h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Outing with</th>
        <th>Activity</th>
        <th>Date</th>
      </tr>

      <% @outings.each do |outing| %>
      <tr>
      <% outinguserid = outing.user_id %>
        <td><a href="/outings/<%= outinguserid %>"><%= @partners.find(outinguserid).user_id_2 %></a></td>
        <!-- <td><%= outing.user_id %></td> -->
        <td><%= outing.activity %></td>
        <td><%= outing.date %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

