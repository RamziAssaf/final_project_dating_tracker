<div class="page-header">
  <h1>Partners</h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Action</th>
        <th></th>
      </tr>

      <% @my_partners.each do |partner| %>
      <tr>
        <td><%= partner.user_id_2 %></td>
        <td>
          <a href="/partners/<%= partner.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_partner/<%= partner.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
          <a href="/outings/<%= partner.id %>" class="btn btn-success">See outings</a>
        </td>
        <td class="hidden"><%= partner.user_id_1 %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<div class="row">
  <a href="/partners/new" class="btn btn-md btn-success">Add Partner</a>
</div>


